{
  "name": "new_carto_version",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@carto/react-api": "^1.1.0",
    "@carto/react-basemaps": "^1.1.0",
    "@carto/react-core": "^1.1.0",
    "@carto/react-redux": "^1.1.0",
    "@carto/react-ui": "^1.1.0",
    "@carto/react-widgets": "^1.1.0",
    "@carto/react-workers": "^1.1.0",
    "@deck.gl/carto": "^8.6.8",
    "@emotion/styled": "^11.8.1",
    "@formatjs/intl-getcanonicallocales": "^1.9.0",
    "@formatjs/intl-locale": "^2.4.45",
    "@formatjs/intl-numberformat": "^7.4.2",
    "@formatjs/intl-pluralrules": "^4.3.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/icons-material": "^5.4.2",
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "deck.gl": "^8.6.8",
    "deepmerge": "^4.2.2",
    "echarts": "^5.3.0",
    "echarts-for-react": "^3.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "history": "^5.2.0",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "js-cookie": "^3.0.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-map-gl": "^5.3.19",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts --max-old-space-size=2048 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{js,jsx}' --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "hygen": "hygen",
    "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "no-console": "warn",
      "no-debugger": "warn"
    },
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 90,
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "yarn lint --max-warnings 0",
      "git add"
    ],
    "*.+(js|jsx|json|css|md)": [
      "yarn format",
      "git add"
    ]
  }
}
